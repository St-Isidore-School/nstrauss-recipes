<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Imports Pearson OnVUE to Munki</string>
	<key>Identifier</key>
	<string>com.github.stisidoreschool.munki.OnVUE</string>
	<key>Input</key>
	<dict>
        <key>NAME</key>
        <string>OnVUE</string>
        <key>SELECTION</key>
        <string>Pearson OnVUE for online test taking</string>
		<key>pkginfo</key>
		<dict>
			<key>catalogs</key>
			<array>
				<string>testing</string>
			</array>
			<key>category</key>
			<string>Productivity</string>
			<key>description</key>
			<string>Pearson InVUE is used for taking exams online</string>
			<key>developer</key>
			<string>Pearson</string>
			<key>display_name</key>
			<string>Pearson OnVUE</string>
			<key>name</key>
			<string>%NAME%</string>
			<key>unattended_install</key>
			<true/>
		</dict>
	</dict>
	<key>MinimumVersion</key>
	<string>0.4.3</string>
	<key>ParentRecipe</key>
	<string>com.github.nstrauss.pkg.OnVUE</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>repo_subdirectory</key>
				<string>%MUNKI_REPO_SUBDIR%</string>
			</dict>
			<key>Processor</key>
			<string>MunkiImporter</string>
		</dict>
	</array>
</dict>
</plist>